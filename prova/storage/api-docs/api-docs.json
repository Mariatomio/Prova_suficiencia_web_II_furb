{
    "openapi": "3.0.0",
    "info": {
        "title": "API de Comandas",
        "version": "1.0.0"
    },
    "paths": {
        "/api/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Autenticação do usuário e obtenção do token",
                "operationId": "a3b306d14572d1f4bd6c064b3233e7b8",
                "requestBody": {
                    "description": "Credenciais do usuário",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "usuario@exemplo.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "senha123"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Login efetuado com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "token": {
                                            "type": "string",
                                            "example": "Bearer eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9..."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Credenciais inválidas"
                    },
                    "422": {
                        "description": "Erro de validação"
                    }
                }
            }
        },
        "/api/logout": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Logout do usuário (invalida o token)",
                "operationId": "fe8f3429cd6979b3b4517e186505f9f9",
                "responses": {
                    "200": {
                        "description": "Logout efetuado com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Logout realizado com sucesso"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Não autorizado"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/api/comandas": {
            "get": {
                "tags": [
                    "Comandas"
                ],
                "summary": "Listar usuários com suas comandas e produtos",
                "operationId": "99f1a009ba10ad39dbb8cbc0c3f1cf15",
                "responses": {
                    "200": {
                        "description": "Lista de usuários",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "idUsuario": {
                                                "type": "integer",
                                                "example": 1
                                            },
                                            "nomeUsuario": {
                                                "type": "string",
                                                "example": "Maria"
                                            },
                                            "telefoneUsuario": {
                                                "type": "string",
                                                "example": "999999999"
                                            }
                                        },
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Não autorizado"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            },
            "post": {
                "tags": [
                    "Comandas"
                ],
                "summary": "Criar uma nova comanda com usuário e produtos",
                "operationId": "6e2b0d567a4d7ccb2be748e5bcb599fe",
                "requestBody": {
                    "description": "Dados do usuário e produtos para a comanda",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "idUsuario": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "nomeUsuario": {
                                        "type": "string",
                                        "example": "Maria"
                                    },
                                    "telefoneUsuario": {
                                        "type": "string",
                                        "example": "999999999"
                                    },
                                    "produtos": {
                                        "type": "array",
                                        "items": {
                                            "required": [
                                                "id",
                                                "nome",
                                                "preco"
                                            ],
                                            "properties": {
                                                "id": {
                                                    "type": "integer",
                                                    "example": 10
                                                },
                                                "nome": {
                                                    "type": "string",
                                                    "example": "Produto A"
                                                },
                                                "preco": {
                                                    "type": "number",
                                                    "format": "float",
                                                    "example": 9.99
                                                }
                                            },
                                            "type": "object"
                                        }
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Comanda criada com sucesso"
                    },
                    "400": {
                        "description": "Dados do produto incompletos"
                    },
                    "401": {
                        "description": "Não autorizado"
                    },
                    "500": {
                        "description": "Erro interno do servidor"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/api/comandas/{id}": {
            "get": {
                "tags": [
                    "Comandas"
                ],
                "summary": "Detalhes do usuário com suas comandas e produtos",
                "operationId": "f7c129099280a3c018903a25b1c2063a",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID do usuário",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 1
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Detalhes do usuário e seus produtos",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "idUsuario": {
                                            "type": "integer",
                                            "example": 1
                                        },
                                        "nomeUsuario": {
                                            "type": "string",
                                            "example": "Maria"
                                        },
                                        "telefoneUsuario": {
                                            "type": "string",
                                            "example": "999999999"
                                        },
                                        "produtos": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "id": {
                                                        "type": "integer",
                                                        "example": 10
                                                    },
                                                    "nome": {
                                                        "type": "string",
                                                        "example": "Produto A"
                                                    },
                                                    "preco": {
                                                        "type": "number",
                                                        "format": "float",
                                                        "example": 9.99
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Usuário não encontrado"
                    },
                    "401": {
                        "description": "Não autorizado"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            },
            "put": {
                "tags": [
                    "Comandas"
                ],
                "summary": "Atualizar os produtos de uma comanda existente",
                "operationId": "d972a823d1c7b28a8b15507f19fb6730",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID da comanda",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 1
                        }
                    }
                ],
                "requestBody": {
                    "description": "Lista de produtos para atualizar na comanda",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "produtos": {
                                        "type": "array",
                                        "items": {
                                            "required": [
                                                "id",
                                                "nome",
                                                "preco"
                                            ],
                                            "properties": {
                                                "id": {
                                                    "type": "integer",
                                                    "example": 10
                                                },
                                                "nome": {
                                                    "type": "string",
                                                    "example": "Produto A"
                                                },
                                                "preco": {
                                                    "type": "number",
                                                    "format": "float",
                                                    "example": 9.99
                                                }
                                            },
                                            "type": "object"
                                        }
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Comanda atualizada com sucesso"
                    },
                    "404": {
                        "description": "Comanda não encontrada"
                    },
                    "401": {
                        "description": "Não autorizado"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            },
            "delete": {
                "tags": [
                    "Comandas"
                ],
                "summary": "Excluir uma comanda",
                "operationId": "3878520a9846b370b9a606417dd7c671",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID da comanda",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 1
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Comanda removida com sucesso"
                    },
                    "404": {
                        "description": "Comanda não encontrada"
                    },
                    "401": {
                        "description": "Não autorizado"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        }
    },
    "components": {
        "securitySchemes": {
            "sanctum": {
                "type": "http",
                "bearerFormat": "API Token",
                "scheme": "bearer"
            }
        }
    },
    "tags": [
